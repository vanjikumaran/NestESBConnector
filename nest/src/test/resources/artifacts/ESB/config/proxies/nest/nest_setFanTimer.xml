<?xml version="1.0" encoding="UTF-8"?>
<proxy xmlns="http://ws.apache.org/ns/synapse"
       name="nest_setFanTimer"
       transports="https,http"
       statistics="disable"
       trace="disable"
       startOnLoad="true">
    <target>
        <inSequence>
            <nest.init/>
            <property name="fanTimerStatus" expression="json-eval($.fanTimerStatus)"/>
            <property name="deviceId" expression="json-eval($.deviceId)"/>
            <nest.setFanTimer>
                <fanTimerStatus>{$ctx:fanTimerStatus}</fanTimerStatus>
                <deviceId>{$ctx:deviceId}</deviceId>
            </nest.setFanTimer>
            <respond/>
        </inSequence>
        <outSequence>
            <log/>
            <send/>
        </outSequence>
    </target>
    <description/>
</proxy>
                                